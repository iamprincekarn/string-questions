string removeConsecutiveCharacter(string s)
    {
        // code here.
        // for(int i=0;i<s.size()-1;i++)
        // {
        //     if(s[i] == s[i+1])
        //     s.erase(i--,1);
        // }
        int i = 0;
        while(i<s.size()-1)
        {
            if(s[i] == s[i+1])
            s.erase(i,1);
            else
            i++;
        }
        return s;
    }

// we can also solve using gfg solutions but i think both are same in terms of complexity and lengthyness
#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    cin>>s;
    unordered_map<char,string>mp;
    mp['a'] = '2';
    mp['b'] = "22";
    mp['c'] = "222";
    mp['d'] = '3';
    mp['e'] = "33";
    //ans so on
    
    string ans;
    for(int i=0;i<s.size();i++)
    {
        ans+=mp[s[i]];
    }
    cout<<ans;
    
    return 0;
}
